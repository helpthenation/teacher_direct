<?xml version="1.0" encoding="utf-8" ?>
<odoo>

  <template id="snippet_ssi" name="SSI_catalog">
    <section class="snippet_ssi">
      <div class="container text-center">
        <div class="row">
          <div class="col-md-1"></div>
          <div class="col-md-2">
            <img height="50px" alt="client" class="img-circle img-responsive" src="/theme_td/static/src/img/placeholder1.jpg"/>
            <!-- <h3>Kyle Name</h3> -->
            <p class="text-secondary text-center">Box</p>
          </div>
          <div class="col-md-2">
            <img height="50px" alt="client" class="img-circle img-responsive" src="/theme_td/static/src/img/placeholder2.jpg"/>
            <!-- <h3>Chad Name</h3> -->
            <p class="text-secondary text-center">Boxes</p>
          </div>
          <div class="col-md-2">
            <img height="50px" alt="client" class="img-circle img-responsive" src="/theme_td/static/src/img/placeholder3.jpg"/>
            <!-- <h3>Jamey Name</h3> -->
            <p class="text-secondary text-center">Science</p>
          </div>
          <div class="col-md-2">
            <img height="50px" alt="client" class="img-circle img-responsive" src="/theme_td/static/src/img/placeholder4.jpg"/>
            <!-- <h3>Chad Name</h3> -->
            <p class="text-secondary text-center">Tapes</p>
          </div>
          <div class="col-md-2">
            <img height="50px" alt="client" class="img-circle img-responsive" src="/theme_td/static/src/img/placeholer5.jpg"/>
            <!-- <h3>Jamey Name</h3> -->
            <p class="text-secondary text-center">Calculator</p>
          </div>
          <div class="col-md-1"></div>
        </div>
      </div>
    </section>
  </template>

  <template id="ssi_snippets" inherit_id="website.snippets" name="SSI_snippets">
    <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
      <t t-snippet="theme_td.snippet_ssi" t-thumbnail="/theme_td/static/src/img/placeholder1.jpg"/>
    </xpath>
  </template>

</odoo>
